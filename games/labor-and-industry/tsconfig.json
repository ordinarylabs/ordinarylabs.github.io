{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipDefaultLibCheck": true,
    "allowJs": false
  },
  "include": ["main.ts", "game.ts"],
  "exclude": ["node_modules"]
}
